{
  "dataList": [
    {
      "id": "0",
      "content": "<b>Zararı Yok</b> \nGölgeri kovalamakla geçen günler"
    },
    {
      "id": "1",
      "content": "Acıklı hatıralardan ibaretler"
    },
    {
      "id": "2",
      "content": "Üzerimden sanki bir ateş geçmiş gibi "
    },
    {
      "id": "3",
      "content": "Elimde tek kalan şey, küller "
    },
    {
      "id": "4",
      "content": "Durmaksızın kaçıp durdum"
    },
    {
      "id": "5",
      "content": "Akan zamanlarda boğuldum"
    },
    {
      "id": "6",
      "content": "Bu halimi de mi arayacağım, diye kendime sordum"
    },
    {
      "id": "7",
      "content": "Şimdiden çok çok uzakta"
    },
    {
      "id": "8",
      "content": "Maziye dönüp baktığımda"
    },
    {
      "id": "9",
      "content": "Umarım ki gülümseyebilirim"
    },
    {
      "id": "10",
      "content": "Geriye ne kadar zamanım kalmışsa"
    },
    {
      "id": "11",
      "content": "İleriye adımımı atmalı"
    },
    {
      "id": "12",
      "content": "Tükenene kadar kendinimi yakmalı"
    },
    {
      "id": "13",
      "content": "Tıpkı bir kandil gibi olmalıyım"
    },
    {
      "id": "14",
      "content": "Nefes almaksızın"
    },
    {
      "id": "15",
      "content": "Boğuldum, günlerde durmaksızın"
    },
    {
      "id": "16",
      "content": "Ümit ki yolumu bulurum"
    },
    {
      "id": "17",
      "content": "Şimdiden çok çok uzakta"
    },
    {
      "id": "18",
      "content": "Maziye dönüp baktığımda"
    },
    {
      "id": "19",
      "content": "Umarım ki gülümseyebilirim"
    },
    {
      "id": "20",
      "content": "Uçayım, uçayım, çok çok uzağa"
    },
    {
      "id": "21",
      "content": "Ki konabileyim kuşlar gibi yıldızlı göğe"
    },
    {
      "id": "22",
      "content": "Mehtaplı gece, mehtaplı gece beni geri bulacak eminim ama..."
    },
    {
      "id": "23",
      "content": "Zararı yok, yaşayabildiğim sürece"
    },
    {
      "id": "24",
      "content": "Zararı yok"
    }
  ]
