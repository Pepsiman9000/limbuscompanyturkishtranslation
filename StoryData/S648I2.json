{
    "dataList": [
        {
            "id": 0,
            "model": "캐서린",
            "content": "Dante."
        },
        {
            "id": 1,
            "model": "캐서린",
            "content": "Senin de kendine ait bir günlük yazdığını görüyorum."
        },
        {
            "id": 2,
            "model": "단테",
            "content": "<…….>"
        },
        {
            "id": 3,
            "model": "단테",
            "content": "<Buna günlük demezdim. Her gün olup biteni kaydetmiyorum...>"
        },
        {
            "id": 4,
            "model": "캐서린",
            "content": "Bu hala bir günlük sayılır."
        },
        {
            "id": 5,
            "model": "캐서린",
            "content": "Bu, hayatının bir kaydı."
        },
        {
            "id": 6,
            "model": "단테",
            "content": "<O zaman... sanırım öyle.>"
        },
        {
            "id": 7,
            "model": "단테",
            "content": "<Catherine.>"
        },
        {
            "id": 8,
            "model": "캐서린",
            "content": "Gün doğmadan hemen önce, günün en sessiz saatlerinde... Günlüğümü yazardım."
        },
        {
            "id": 9,
            "model": "캐서린",
            "content": "Hele ki ateşler içinde olduğum günlerde, her kusurumu bir kağıda kusmadan devam edemezdim."
        },
        {
            "id": 10,
            "model": "캐서린",
            "content": "Ben günlüğümle hep dürüsttüm."
        },
        {
            "id": 11,
            "model": "캐서린",
            "content": "Fakat..."
        },
        {
            "id": 12,
            "model": "캐서린",
            "content": "Sanırım başka hiçbir yerde dürüst olamadım."
        },
        {
            "id": 13,
            "model": "캐서린",
            "content": "Sevdiklerimden gerçek duygularımı gizlerken... kendimi ifade etmeyi reddederken... beni anlayamayanlara öfkelendim."
        },
        {
            "id": 14,
            "model": "캐서린",
            "content": "Heathcliff asla bilmemeli."
        },
        {
            "id": 15,
            "model": "캐서린",
            "content": "O asla benim ne kadar..."
        },
        {
            "id": 16,
            "model": "캐서린",
            "content": "Benim ne kadar..."
        },
        {
            "id": 17,
            "model": "히스클리프",
            "content": "Hayır. Bilmeliyim"
        },
        {
            "id": 18,
            "model": "캐서린",
            "content": "Heathcliff? Sen nasıl...?"
        },
        {
            "id": 19,
            "model": "히스클리프",
            "content": "Buraya gelebilmek için çok cesarete ihtiyacım vardı."
        },
        {
            "id": 20,
            "model": "히스클리프",
            "content": "Catherine."
        },
        {
            "id": 21,
            "model": "히스클리프",
            "content": "Senin için her şey çok kolay. Beni devirmen..."
        },
        {
            "id": 22,
            "model": "히스클리프",
            "content": "Beni hiçbir şeye dönüştürene kadar beni cezalandırman."
        },
        {
            "id": 23,
            "model": "히스클리프",
            "content": "Beni ziyarete geldiğinde çok mutlu olurdum. Bana doğru tek bir bakışın tüm günümü aydınlatabilirdi. Neredeyse bir felaket gibiydin."
        },
        {
            "id": 24,
            "model": "히스클리프",
            "content": "Ve ben her zaman, her zaman..."
        },
        {
            "id": 25,
            "model": "히스클리프",
            "content": "... beni fırtına gibi saran o felaketi her zaman sevdim, Catherine."
        },
        {
            "id": 26,
            "model": "캐서린",
            "content": "……."
        },
        {
            "id": 27,
            "model": "캐서린",
            "content": "Teşekkür ederim. Cesarete sahip olduğun için."
        },
        {
            "id": 28,
            "model": "캐서린",
            "content": "Nihayet, kalplerimiz..."
        },
        {
            "id": 29,
            "model": "캐서린",
            "content": "... kırık değil."
        },
        {
            "id": 30,
            "model": "캐서린",
            "content": "Dante.  Günlüğünü bir dakika ödünç alabilir miyim?"
        },
        {
            "id": 31,
            "model": "캐서린",
            "content": "Altın Dal, hepimizin hayal edebileceğinden çok daha etkileyici güçlere sahip."
        },
        {
            "id": 32,
            "model": "단테",
            "content": "<Hangi güçler...?>"
        },
        {
            "id": 33,
            "model": "캐서린",
            "content": "Kendi isteklerini gerçeğe dönüştürüyor, değil mi?"
        },
        {
            "id": 34,
            "model": "캐서린",
            "content": "Yani..."
        },
        {
            "id": 35,
            "model": "캐서린",
            "content": "Heathcliff'imin mutluluğunun anahtarı..."
        },
        {
            "id": 36,
            "model": "캐서린",
            "content": "……."
        },
        {
            "id": 37,
            "model": "캐서린",
            "content": "... günlüğünde yer almakta."
        },
        {
            "id": 38,
            "content": "Ben..."
        },
        {
            "id": 39,
            "content": "... taşıdığım cihazın ekranına baktım. Eskiden sadece bir şeyleri kaydetmek için kullanılan bir araç olduğunu düşündüğüm bir şeydi."
        },
        {
            "id": 40,
            "model": "캐서린",
            "content": "İlk konuşan kişi olduğun için çok teşekkür ederim, Heathcliff."
        },
        {
            "id": 41,
            "model": "캐서린",
            "content": "Her dünyadaki her Heathcliff'i sevmiyorum."
        },
        {
            "id": 42,
            "model": "캐서린",
            "content": "Seni seviyorum. Şu an olduğun gibi."
        }
    ]
}
